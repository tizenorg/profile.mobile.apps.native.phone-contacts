#define COLORBOX(n,rr,gg,bb,opa) \
	part { \
		name: n".color"; \
		type: RECT; \
		scale: 1; \
		description { \
			state: "default" 0.0; \
			color: rr gg bb opa; \
			rel1 { relative: 0.0 0.0; to: n;} \
			rel2 { relative: 1.0 1.0; to: n;} \
		} \
	}

