#define PADDING_TOP( param_padding_size ) \
	part { \
		name: "padding.top"; \
		type: SPACER; \
		scale: 1; \
		description { \
			state: "default" 0.0; \
			min: 0 param_padding_size; \
			max: -1 param_padding_size; \
			fixed: 0 1; \
			rel2.relative: 1.0 0.0; \
			align: 0.5 0.0; \
		} \
	}

#define PADDING_BOTTOM( param_padding_size ) \
	part { \
		name: "padding.bottom"; \
		type: SPACER; \
		scale: 1; \
		description { \
			state: "default" 0.0; \
			min: 0 param_padding_size; \
			max: -1 param_padding_size; \
			fixed: 0 1; \
			rel1.relative: 0.0 1.0; \
			align: 0.5 1.0; \
		} \
	}

